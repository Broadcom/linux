	pmi-bus {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;

		pcie_phy: phy {
			compatible = "brcm,sr-pcie-phy";
			brcm,sr-cdru = <&cdru>;
			brcm,sr-mhb = <&mhb>;
			#phy-cells = <1>;
		};
	};
